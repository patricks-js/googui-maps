{
  "name": "googui-maps",
  "version": "1.0.0",
  "description": "A simulator of google maps.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --env-file=.env.local src/main/server.js",
    "dev": "node --watch --env-file=.env.local src/main/server.js",
    "lint": "biome check --write src",
    "lint:fix": "biome check --write --unsafe src",
    "docker:compose": "docker-compose --env-file .env.local",
    "services:up": "pnpm run docker:compose up -d",
    "services:stop": "pnpm run docker:compose stop",
    "services:down": "pnpm run docker:compose down",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.14.0",
    "@scalar/fastify-api-reference": "^1.24.39",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "mongoose": "^8.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@vitest/coverage-istanbul": "^2.0.4",
    "lefthook": "^1.7.1",
    "supertest": "^7.0.0",
    "vitest": "^2.0.4"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}